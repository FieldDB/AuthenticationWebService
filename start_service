#!/bin/bash

cd $HOME/fielddbworkspace/AuthenticationWebService
bash switch_to_producti.sh 

$HOME/fielddbworkspace/AuthenticationWebService/node_modules/forever/bin/forever \
start \
-al $HOME/fielddbworkspace/logs/fielddbauth.log \
-ao $HOME/fielddbworkspace/logs/fielddbauthout.log \
-ae $HOME/fielddbworkspace/logs/fielddbautherr.log \
service.js
