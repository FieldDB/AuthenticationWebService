{
  "name": "as-auth-provider",
  "version": "0.0.1",
  "description": "Example auth provider for authenticated systems workshop",
  "scripts": {
    "coverage": "istanbul cover _mocha -- --recursive test; istanbul check-coverage  --statements 90 --functions 90 --branches 77 --lines 90",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "jshint config models middleware routes test service.js",
    "postinstall": "bower install",
    "start": "node ./bin/www",
    "test": "mocha --recursive test",
    "test:debug": "node-debug _mocha test/integration/oauth.js"
  },
  "main": "service",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.1",
    "body-parser": "^1.14.2",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-handlebars": "^3.0.0",
    "express-oauth-server": "https://github.com/oauthjs/express-oauth-server.git",
    "jsonwebtoken": "^7.0.1",
    "morgan": "^1.6.1",
    "node-uuid": "^1.4.7",
    "oauth2-server": "https://github.com/cesine/node-oauth2-server.git",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "sequelize": "^3.23.1",
    "sqlite3": "^3.1.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0"
  },
  "license": "MIT"
}
